{
  "version": 2,
  "headers": [
    {
      "source": "/functions",
      "headers": [
        { "key": "access-control-allow-origin", "value": "*" }
      ]
    }
  ],
  "builds": [
    {
      "src": "src/api.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "(.*)",
      "dest": "src/api.js"
    }
  ]
}