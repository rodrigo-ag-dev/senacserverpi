{
  "version": 2,
  "builds": [
    {
      "src": "src/api.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "(.*)",
      "dest": "src/api.js""headers": [
        {
          "source": "/functions",
          "headers": [
            {
              "key": "access-control-allow-origin",
              "value": "*"
            }
          ]
        }
      ]
    }
  ]
}